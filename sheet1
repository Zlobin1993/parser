<script src="/assets/js/bootstrap-datetimepicker.js"></script>
<script src="/assets/js/locales/bootstrap-datetimepicker.ru.js"></script>
<link rel="stylesheet" type="text/css" href="/assets/css/datetimepicker.css">

<script type="text/javascript">

$(document).ready(function() {

    $(".advanced-search").click(function() {

        $('.advanced-search-form').toggleClass('hide');

    });

    // Дата окончания подачи заявок от
    $('#offer_from').datetimepicker({
        format : 'dd.mm.yyyy',
        weekStart: 1,
        language: 'ru',
        minView : 2,
        autoclose: true
    });

    // Дата окончания подачи заявок до
    $('#offer_to').datetimepicker({
        format : 'dd.mm.yyyy',
        weekStart: 1,
        language: 'ru',
        minView : 2,
        autoclose: true
    });


    // Дата публикации извещения от
    $('#published_from').datetimepicker({
        format : 'dd.mm.yyyy',
        weekStart: 1,
        language: 'ru',
        minView : 2,
        autoclose: true
    });

    // Дата публикации извещения до
    $('#published_to').datetimepicker({
        format : 'dd.mm.yyyy',
        weekStart: 1,
        language: 'ru',
        minView : 2,
        autoclose: true
    });

	$(".reset").click(function() {

		$('select option:first').attr('selected', 'selected');
		
	    $(this).closest('form').find("input[type=text], textarea").val("");
	    $(this).closest('form').submit();
	});

});

</script></div>

